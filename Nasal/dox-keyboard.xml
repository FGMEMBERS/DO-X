<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <key n="100">
    <name>d</name>
    <desc>Canopy</desc>
    <binding>
     <command>nasal</command>
     <script>doors.doorsystem.crewexport();</script>
    </binding>
    <binding>
     <command>property-toggle</command>
     <property>/instrumentation/generic/click</property>
    </binding>
  </key>

  <key n="115">
    <name>s</name>
    <desc>Start Engines</desc>
    <binding>
     <command>nasal</command>
     <script>dox.startEngines();</script>
    </binding>
  </key>

  <key n="19">
    <name>Ctrl-S</name>
    <desc>Stop Engine</desc>
    <binding>
      <command>nasal</command>
      <script>dox.stopEngines();</script>
    </binding>
  </key>

  <key n="57">
    <name>PgUp</name>
   <desc>Throttle</desc>
   <binding>
    <command>nasal</command>
    <script>dox.throttleControl(0.01,1.0)</script>
    </binding>
  </key>

  <key n="51">
    <name>PgDown</name>
   <desc>Throttle</desc>
   <binding>
    <command>nasal</command>
    <script>dox.throttleControl(-0.01,1.0)</script>
    </binding>
  </key>

  <key n="44">
    <name>,</name>
    <repeatable>true</repeatable>
    <desc>Throttle Left Up</desc>
    <binding>
      <command>nasal</command>
      <script>dox.throttleControlL(0.01,1.0)</script>
    </binding>
  </key>

  <key n="59">
    <name>.</name>
    <repeatable>true</repeatable>
    <desc>Throttle Left Down</desc>
    <binding>
      <command>nasal</command>
      <script>dox.throttleControlL(-0.01,1.0)</script>
    </binding>
  </key>

  <key n="46">
    <name>;</name>
    <repeatable>true</repeatable>
    <desc>Throttle Right Up</desc>
    <binding>
      <command>nasal</command>
      <script>dox.throttleControlR(0.01,1.0)</script>
    </binding>
  </key>

  <key n="58">
    <name>:</name>
    <repeatable>true</repeatable>
    <desc>Throttle Right Down</desc>
    <binding>
      <command>nasal</command>
      <script>dox.throttleControlR(-0.01,1.0)</script>
    </binding>
  </key>

  <key n="66">
    <name>B</name>
    <desc>Anchor down</desc>
    <binding>
      <command>nasal</command>
      <script>dox.castAnchor();</script>
    </binding>
  </key>

  <key n="98">
    <name>b</name>
    <desc>Anchor up</desc>
    <binding>
      <command>nasal</command>
      <script>dox.hoistAnchor();</script>
    </binding>
  </key>

  <key n="108">
    <name>l</name>
    <desc>Interior Lights on/off</desc>
    <binding>
      <command>nasal</command>
      <script>dox.interiorLights();</script>
    </binding>
  </key>

  <key n="9">
    <name>Ctrl-I</name>
    <desc>Do X Engineer Menu</desc>
    <binding>
      <command>dialog-show</command>
      <dialog-name>dox-engineer</dialog-name>
    </binding>
  </key>

  <key n="60">
    <name> > </name>
    <desc>View of the Radio Navigation Officer</desc>
    <binding>
      <command>nasal</command>
      <script>dox.changeView(13);</script>
    </binding>
  </key>

</PropertyList>
