<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>inttour</object-name>
    <object-name>mur1</object-name>
    <object-name>mur2</object-name>
    <object-name>planchets</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- Slats / Lattenrost -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/Slats/slats.xml</path>
    <offsets>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </offsets>
  </model>

  <!-- Caisse dans NavigationCabin  -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/NavigationCabin/caisse.xml</path>
    <offsets>
      <x-m> -9.500 </x-m>
      <y-m> -0.720 </y-m>
      <z-m> -0.658 </z-m> <!-- up -->
    </offsets>
  </model>

  <!-- Table dans NavigationCabin  -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/NavigationCabin/table.xml</path>
    <offsets>
      <x-m> -10.205 </x-m>
      <y-m>  -1.060 </y-m>
      <z-m>   0.000 </z-m> <!-- up -->
      <roll-deg>      0 </roll-deg>
      <pitch-deg>     0 </pitch-deg>
      <heading-deg>  -2 </heading-deg>
    </offsets>
  </model>

  <!-- Pilote -->
  <model>
    <path>Aircraft/DO-X/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -11.480 </x-m>
      <y-m>  -0.695 </y-m>
      <z-m>  -0.090 </z-m>
    </offsets>
  </model>

  <!-- Cockpit enviroment -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -12.249 </x-m>
      <y-m>   0.000 </y-m>
      <z-m>  -0.352 </z-m>
    </offsets>
  </model>

  <!-- Engineer enviroment -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/EngineerPanel/right_engineerpanel.xml</path>
    <offsets>
      <x-m> -7.000 </x-m>
      <y-m>  0.850 </y-m>
      <z-m>  0.380 </z-m>
      <roll-deg>      0 </roll-deg>
      <pitch-deg>     0 </pitch-deg>
      <heading-deg> -90 </heading-deg>
    </offsets>
  </model>

  <model>
    <path>Aircraft/DO-X/Models/Interior/EngineerPanel/left_engineerpanel.xml</path>
    <offsets>
      <x-m> -7.000 </x-m>
      <y-m> -0.850 </y-m>
      <z-m>  0.380 </z-m>
      <roll-deg>     0 </roll-deg>
      <pitch-deg>    0 </pitch-deg>
      <heading-deg> 90 </heading-deg>
    </offsets>
  </model>

</PropertyList>

