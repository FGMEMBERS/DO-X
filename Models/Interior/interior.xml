<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>inttour</object-name>
    <object-name>mur1</object-name>
    <object-name>mur2</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>intvitres</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>intvitres</object-name>
  </animation>

  <!-- Pilote -->
  <model>
    <path>Aircraft/DO-X/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -11.472 </x-m>
      <y-m>  -0.540 </y-m>
      <z-m>  -0.500 </z-m>
    </offsets>
  </model>

  <!-- Cockpit enviroment -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -12.249 </x-m>
      <y-m>   0.000 </y-m>
      <z-m>  -0.352 </z-m>
    </offsets>
  </model>

</PropertyList>

