<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>aft-ger.ac</path>

  <animation>
    <object-name>rebord</object-name>
    <object-name>spirit_bottom</object-name>
    <object-name>spirit_column</object-name>

    <object-name>glas_tube</object-name>
  </animation>

  <animation> 
    <type>scale</type>
    <object-name>spirit_column</object-name>
    <property>/orientation/pich-deg</property>
    <x-offset> 1 </x-offset>
    <y-offset> 1 </y-offset>
    <z-offset> 1 </z-offset>
    <x-factor> 0.000 </x-factor>
    <y-factor> 0.000 </y-factor>
    <z-factor> 0.045 </z-factor>
    <z-min> 0.01 </z-min>
    <z-max> 2.01 </z-max>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.056 </z-m>
    </center>
  </animation>

  <!--
  animation> 
    <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
      <red>   0.75 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation
  -->

  <animation>
    <type>select</type>
    <object-name>spirit_column</object-name>
    <object-name>spirit_bottom</object-name>
    <condition>
      <property>/orientation/pich-deg</property>
    </condition>
  </animation>

  <animation>
    <type>noshadow</type>
  </animation>

</PropertyList>

