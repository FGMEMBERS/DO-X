    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.111 </x-m>
      <y-m>  0.770 </y-m>
      <z-m>  0.325 </z-m>
      <pitch-deg> -8 </pitch-deg>
    </offsets>
  </model>

  <!-- ****************************** MIDDLE-(ENGINE-CONTROL)-PANEL *********************************-->
  <!-- Moteurs extérieur gauche avant --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/EngineerPanel/Instruments/clutchctrl/clutchctrl-0.xml</path>
    <offsets>
      <x-m>  0.230 </x-m>
      <y-m> -0.300 </y-m>
      <z-m> -0.135 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg> -33 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>
  <!-- Moteurs extérieur gauche arrière -->
  <model>
