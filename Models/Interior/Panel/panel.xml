      <roll-deg>    0 </roll-deg>
      <pitch-deg> -33 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>
  <!-- Moteur intérieur gauche arrière -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/EngineerPanel/Instruments/clutchctrl/clutchctrl-10.xml</path>
    <offsets>
      <x-m>  0.280 </x-m>
      <y-m> -0.200 </y-m>
      <z-m> -0.210 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg> -33 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

  <!-- Average RPM for the locked (clutch on)  larboard engines -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/EngineerPanel/Instruments/rpm/rpmL.xml</path>
    <offsets>
      <x-m>  0.260 </x-m>
      <y-m> -0.080 </y-m>
      <z-m> -0.170 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg> -33 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>
  
  <!-- Average RPM for the locked (clutch on)  starboard engines -->
  <model>
