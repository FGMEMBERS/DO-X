  <!--************************ Text on the Panel **************************-->
  <animation>
    <type>material</type>
    <object-name>Registration</object-name>
    <emission>
        <red>1.0</red>
        <green>1.0</green>
        <blue>1.0</blue>
    </emission>
  </animation>

  <text>
    <name>Registration</name>
    <type type="string">text-value</type>
    <property type="string">/sim/multiplay/callsign</property>
    <format type="string">%s</format>
    <draw-text type="bool">true</draw-text>
    <font>Helvetica.txf</font>
    <character-size type="double">0.016</character-size>
    <character-aspect-ratio type="double">1.0</character-aspect-ratio>
    <axis-alignment>yz-plane</axis-alignment>
    <max-height>0.2</max-height>
    <max-width>0.5</max-width>
    <font-resolution>
        <width type="int">32</width>
        <height type="int">32</height>
    </font-resolution>
    <alignment>center-center</alignment>
    <offsets>
      <x-m>  0.085 </x-m> <!-- near to panel -->
      <y-m> -0.620 </y-m> <!-- right -->
      <z-m>  0.455 </z-m> <!-- up -->
      <pitch-deg> -8 </pitch-deg>
    </offsets>
  </text>

<!-- Labels activated by hotspots -->

  <animation>
    <type>material</type>
    <object-name>LabelRDF</object-name>
    <object-name>LabelAI</object-name>
    <object-name>LabelMiddle</object-name>
    <emission>
        <red>1.0</red>
        <green>0.8</green>
        <blue>0.0</blue>
    </emission>
  </animation>

  <animation>
    <type>select</type>
    <object-name>LabelRDF</object-name>
    <object-name>LabelAI</object-name>
    <object-name>LabelMiddle</object-name>
    <condition>
        <property>/sim/panel-hotspots</property>
    </condition>
  </animation>

  <!-- Radio Direction Finder on FrontDeck --> 
  <text>
    <name>LabelRDF</name>
    <type type="string">literal</type>
    <text type="string">Radio Direction Finder (use ADF frequency)</text>
    <draw-text type="bool">true</draw-text>
    <font>Helvetica.txf</font>
    <character-size type="double">0.1</character-size>
    <character-aspect-ratio type="double">1.0</character-aspect-ratio>
    <axis-alignment>yz-plane</axis-alignment>
    <max-height>0.8</max-height>
    <max-width>1.1</max-width>
    <font-resolution>
        <width type="int">32</width>
        <height type="int">32</height>
    </font-resolution>
    <alignment>center-center</alignment>
    <offsets>
      <x-m> -3.200 </x-m> <!-- near to panel -->
      <y-m>  0.830 </y-m> <!-- right -->
      <z-m>  0.570 </z-m> <!-- up -->
    </offsets>
  </text>

  <!-- Fluglage zentral --> 
  <text>
    <name>LabelAI</name>
    <type type="string">literal</type>
    <text type="string">Altitude Indicator</text>
    <draw-text type="bool">true</draw-text>
    <font>Helvetica.txf</font>
    <character-size type="double">0.02</character-size>
    <character-aspect-ratio type="double">1.0</character-aspect-ratio>
    <axis-alignment>yz-plane</axis-alignment>
    <max-height>0.2</max-height>
    <max-width>0.1</max-width>
    <font-resolution>
        <width type="int">32</width>
        <height type="int">32</height>
    </font-resolution>
    <alignment>center-center</alignment>
    <offsets>
      <x-m> -0.330 </x-m> <!-- near to panel -->
      <y-m>  0.000 </y-m> <!-- right -->
      <z-m>  0.082 </z-m> <!-- up -->
      <pitch-deg> -8 </pitch-deg>
    </offsets>
  </text>

  <!-- RPM on Middle Panel --> 
  <text>
    <name>LabelMiddle</name>
    <type type="string">literal</type>
    <text type="string">Clutch Control Lights for every Engine. Two instruments with the average RPM of the locked lar- and starboard engines</text>
    <draw-text type="bool">true</draw-text>
    <font>Helvetica.txf</font>
    <character-size type="double">0.018</character-size>
    <character-aspect-ratio type="double">1.0</character-aspect-ratio>
    <axis-alignment>yz-plane</axis-alignment>
    <max-height>0.2</max-height>
    <max-width>0.2</max-width>
    <font-resolution>
        <width type="int">32</width>
        <height type="int">32</height>
    </font-resolution>
    <alignment>center-center</alignment>
    <offsets>
      <x-m>  0.250 </x-m> <!-- near to panel -->
      <y-m>  0.000 </y-m> <!-- right -->
      <z-m>  0.082 </z-m> <!-- up -->
      <pitch-deg> -15 </pitch-deg>
    </offsets>
  </text>


</PropertyList>

