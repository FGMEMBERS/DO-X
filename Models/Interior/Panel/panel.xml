      <y-m> -0.300 </y-m>
      <z-m> -0.210 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg> -33 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>


  <!-- Moteur milieu gauche avant -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/EngineerPanel/Instruments/clutchctrl/clutchctrl-2.xml</path>
    <offsets>
      <x-m>  0.230 </x-m>
