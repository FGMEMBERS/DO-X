<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>
    <object-name>leather</object-name>
    <object-name>visses</object-name>
    
    <!-- Objets transparents -->
  </animation>

  <!-- *********  Central-Position of the Instruments in the front of the Cockpit ************ -->
  <!-- UHR Central --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/clock/clock.xml</path>
    <offsets>
      <x-m> -0.415 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.210 </z-m>
    </offsets>
  </model>

  <!-- FLUGLAGE Central --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/att/att-big.xml</path>
    <offsets>
      <x-m> -0.460 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.020 </z-m>
    </offsets>
  </model>

  <!-- Yokes --> 
  <!-- Yoke Pilot --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Yokes/yoke.xml</path>
    <offsets>
      <x-m>  0.650 </x-m> <!-- near to panel -->
      <y-m> -0.670 </y-m> <!-- right -->
      <z-m> -0.420 </z-m> <!-- up -->
    </offsets>
  </model>
  
  <!-- Yoke Copilot -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Yokes/yoke.xml</path>
    <offsets>
      <x-m>  0.650 </x-m> <!-- near to panel -->
      <y-m>  0.670 </y-m> <!-- right -->
      <z-m> -0.420 </z-m> <!-- up -->
    </offsets>
  </model>
  
  <!-- Yokeboot -->
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Yokes/yokeboot.xml</path>
    <offsets>
      <x-m>  0.650 </x-m> <!-- near to panel -->
      <y-m>  0.000 </y-m> <!-- right -->
      <z-m> -0.420 </z-m> <!-- up -->
    </offsets>
  </model>

  <!-- ******************************* PILOT-PANEL **********************************-->
  <!-- Positions on the Panel  -->
  <!--             1           -->
  <!--       2   3   4   5.    -->
  <!--       6.          7.    -->

  <!-- 1. MAGNETIC COMPASS --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/compass/compass_magnetic.xml</path>
    <offsets>
      <x-m>  0.020 </x-m>
      <y-m> -0.620 </y-m>
      <z-m>  0.565 </z-m>
    </offsets>
  </model>

  <!-- 2. SPEED --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m>  0.090 </x-m>
      <y-m> -0.785 </y-m>
      <z-m>  0.455 </z-m>
      <pitch-deg> -8 </pitch-deg>
    </offsets>
  </model>

  <!-- 3. VERTICAL SPEED --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/vario/vario.xml</path>
    <offsets>
      <x-m>  0.090 </x-m> <!-- near to panel -->
      <y-m> -0.675 </y-m> <!-- right -->
      <z-m>  0.455 </z-m> <!-- up -->
      <pitch-deg> -8 </pitch-deg>
    </offsets>
  </model>

  <!-- 4. TURN --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/turn/turn.xml</path>
    <offsets>
      <x-m>  0.090 </x-m>
      <y-m> -0.575 </y-m>
      <z-m>  0.455 </z-m>
      <pitch-deg> -8 </pitch-deg>
    </offsets>
  </model>

  <!-- 5. HI / KURS Gyro --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/hi/hi.xml</path>
    <offsets>
      <x-m>  0.086 </x-m>
      <y-m> -0.447 </y-m>
      <z-m>  0.442 </z-m>
      <pitch-deg> -10 </pitch-deg>
    </offsets>
  </model>

  <!-- 6. ALT --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.111 </x-m>
      <y-m> -0.770 </y-m>
      <z-m>  0.325 </z-m>
      <pitch-deg> -8 </pitch-deg>
    </offsets>
  </model>

  <!-- 7. AFN --> 
  <model>
    <path>Aircraft/DO-X/Models/Interior/Panel/Instruments/afn2/afn2.xml</path>
    <offsets>
      <x-m>  0.115 </x-m>
      <y-m> -0.447 </y-m>
      <z-m>  0.308 </z-m>
      <pitch-deg> -8 </pitch-deg>
    </offsets>
  </model>

  <!-- ****************************** COPILOT-PANEL *********************************-->
  <!-- Positions on the Copilot-Panel -->
  <!--               11            -->
  <!--       12   13   14   15.    -->
  <!--       16.            17.    -->

